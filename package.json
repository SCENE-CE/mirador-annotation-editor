{
  "name": "mirador-annotation-editor",
  "version": "1.0.75",
  "description": "mirador-annotation-editor plugin in a React component. Mirador 4 (Alpha 2) compatible ",
  "main": "es/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "npm run build:es",
    "build:umd": "NODE_ENV=production webpack --mode=production",
    "build:es": "mkdir -p es && cp -r src/* es && NODE_ENV=production MODULE_FORMAT=es npx babel es -d es",
    "build:cjs": "mkdir -p lib && cp -r src/* lib && NODE_ENV=production MODULE_FORMAT=cjs npx babel lib -d lib",
    "build:demo": "NODE_ENV=development webpack --mode=development",
    "clean": "rm -rf ./umd && rm -rf ./es && rm -rf ./lib && rm -rf ./demo/dist",
    "clean-deps": "rm -rf package-lock.json && rm -rf node_modules",
    "lint": "eslint ./src ./__tests__",
    "prepare": "npm run build",
    "prepublishOnly": "npm run clean && npm run build:es",
    "start": "NODE_ENV=development webpack serve --open",
    "test": "npm run lint && jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --watchAll=false"
  },
  "contributors": [
    "Anthony Geourjon <anthony.geourjon@tetras-libre.fr> (https://www.tetras-libre.fr)",
    "David Rouquet <david.rouquet@tetras-libre.fr> (https://www.tetras-libre.fr)",
    "Antoine Roy <antoine.roy@tetras-libre.fr> (https://www.tetras-libre.fr)"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/system": "^5.15.1",
    "axios": "^1.6.7",
    "babel-loader": "^9.1.3",
    "brace": "^0.11.1",
    "draft-js": "^0.11.6",
    "draft-js-export-html": "^1.4.1",
    "draft-js-import-html": "^1.4.1",
    "html-loader": "^5.0.0",
    "i18next": "^24.2.0",
    "jsoneditor": "^9.10.5",
    "jsoneditor-react": "npm:jsoneditor-react18@0.5.0",
    "react-color": "^2.18.1",
    "react-i18next": "^15.4.0",
    "react-konva": "^18.0.0",
    "react-konva-to-svg": "^1.0.2",
    "react-quill": "^2.0.0",
    "react-redux": "8.1.3",
